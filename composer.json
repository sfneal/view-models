{
    "name": "sfneal/view-models",
    "description": "Boilerplate for PHP package repo's",
    "keywords": [
        "sfneal",
        "view-models"
    ],
    "homepage": "https://github.com/sfneal/view-models",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Stephen Neal",
            "email": "stephen.neal14@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": ">=6.0",
        "sfneal/redis-helpers": ">=0.4.0",
        "sfneal/string-helpers": ">=0.2.0",
        "spatie/laravel-view-models": ">=1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0"
    },
    "autoload": {
        "psr-4": {
            "Sfneal\\ViewModels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sfneal\\ViewModels\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
